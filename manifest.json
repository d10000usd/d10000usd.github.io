

  {
    "manifest_version": 3,
    "name": "coin-v",
    "version": "1.0.0",
    "description": "Ratio change monitoring",
    "background": {
      "service_worker": "service-worker.js"
    },
    "action": {
      "default_title": "Click to open panel"
    },
    "permissions": ["sidePanel", "tabs"],
    "commands": {
      "_execute_action": {
        "suggested_key": {
          "default": "Ctrl+B",
          "mac": "Command+B"
        }
      }
    },
    "icons": {
      "16": "images/bolb-128.png",
      "48": "images/bolb-128.png",
      "128": "images/bolb-128.png"
    }
  }
  